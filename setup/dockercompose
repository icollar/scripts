curl -L https://github.com/docker/compose/releases/download/1.3.2/docker-compose-`uname -s`-`uname -m` > docker-compose
sudo mv docker-compose /usr/local/bin/docker-compose
sudo chmod +x /usr/local/bin/docker-compose
curl -L https://raw.githubusercontent.com/docker/compose/1.3.2/contrib/completion/bash/docker-compose > docker-compose
sudo mv docker-compose /etc/bash_completion.d/docker-compose

# vim:ft=sh
